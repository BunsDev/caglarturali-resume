    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*\"",
    "resume": "tsx scripts/processResume.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
